{
  "name": "@prisma-cms/webrtc",
  "description": "WebRTC module for @prisma-cms",
  "version": "1.4.2",
  "private": false,
  "main": "lib/App.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build-less && nwb build --copy-files",
    "build-app": "react-scripts build",
    "build-less": "node-less-chokidar src/styles/less",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@fi1osof/rtcmulticonnection-v3": "^3.4.4",
    "@prisma-cms/context": "latest",
    "@prisma-cms/society": "latest",
    "graphql": "14.0.2",
    "rtcmulticonnection": "^3.6.8",
    "webrtc-adapter": "latest"
  },
  "devDependencies": {
    "@prisma-cms/front": "latest",
    "@prisma-cms/front-editor": "latest",
    "@prisma-cms/performance": "latest",
    "graphql-cli": "2.17.0",
    "node-less-chokidar": "^0.3.1",
    "nwb": "^0.23.0",
    "react-scripts": "2.1.2",
    "source-map-explorer": "^2.0.1"
  },
  "resolutions": {
    "graphql": "14.0.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "prisma-cms",
    "front",
    "components"
  ],
  "author": "Fi1osof",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prisma-cms/webrtc.git"
  },
  "bugs": {
    "url": "https://github.com/prisma-cms/webrtc/issues"
  },
  "homepage": "https://github.com/prisma-cms/webrtc#readme"
}
